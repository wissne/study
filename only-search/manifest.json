{
    "manifest_version": 2,
    "name": "Only Search",
    "version": "1.9",
    "omnibox": {
        "keyword": "os"  //地址栏输入 u> 空格或 Tab 进入本模式
    },
    "permissions": [
        "tabs",
        "http://*/*",
         "https://*/*"
    ],    
    "icons": {  //可选，没有话，Chrome 会使用默认的图标
        "16": "search-16.png" //需准备好这个文件
    },
    "background": {
        "persistent": false,
        "scripts": ["jquery-2.1.3.min.js", "background.js"] //程序就写在这里面
    }
}